            <string>///START

//create a path for a missile
missile_path = path_add();

path_add_point(missile_path, x, y, mov_speed); //starting point

var turn_around_path = path_duplicate(path_missile); //copy turn around path

path_shift(turn_around_path, irandom_range(-32, 32), irandom_range(-32, 32)); //shift the turn around

var iid = id | 0;
if (iid mod 2) &gt; 0 then path_mirror(turn_around_path);

path_append(missile_path, turn_around_path); //append turn around

var tcx = target.x + target.atom_w_half;
var tcy = target.y + target.atom_h_half;
path_add_point(missile_path, tcx, tcy, 100); //add end point

path_set_kind(missile_path, 1); //smooth
path_set_precision(missile_path, 4);
path_set_closed(missile_path, false); //make sure it is an open path

//start the path
path_start(missile_path, mov_speed, path_action_stop, true);

//path_speed = 50;
visible = true;
scr_play_snd(Sound.Missile);



</string>
