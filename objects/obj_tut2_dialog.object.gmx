bs_alpha += .05;
if bs_alpha &gt; 1 then bs_alpha = 1;

switch(state)
{
    case WEB_TUT2.RESET_FADE_OUT:
       
        break;
        
    case WEB_TUT2.RESET_FADE_IN:
        with(lightning)
        {           
            image_alpha += .05;
            if image_alpha &gt; 1 then image_alpha = 1;
            if image_alpha == 1 then other.state = WEB_TUT2.POINTER_FADE_IN;
        }
        
        with(obj_atom_lightning)
        {
            image_alpha -= .05;
            if image_alpha &lt; 0 then image_alpha = 0;
        }
      
        
        
        break;
    case WEB_TUT2.POINTER_FADE_OUT:
        with(pointer)
        {
            image_alpha -= .08;
            if image_alpha &lt; 0 
            {
                x = xstart;
                y = ystart;
                visible = false;
                image_alpha = 0;
            }
           // if image_alpha == 0 then other.state = WEB_TUT1.RESET_FADE_IN;
        }
        break;
    case WEB_TUT2.POINTER_FADE_IN:
        with(pointer)
        {
            y = ystart;
            x = xstart;
            visible = true;
            image_alpha += .08;
            if image_alpha &gt; 1 then image_alpha = 1;
            
            if image_alpha == 1 
            {
                other.alarm[5] = 12; //START CLICK
                other.state = WEB_TUT2.IDLE;           
            }
        }
        break;
}
