            <string>
bs_alpha += .05;
if bs_alpha &gt; 1 then bs_alpha = 1;

switch(state)
{
    case WEB_TUT3.RESET_FADE_OUT:
       
        break;
        
    case WEB_TUT3.RESET_FADE_IN:
    
        with(obj_tut3_isotopes)
        {                  
            image_alpha += .05;
            if image_alpha &gt; 1 then image_alpha = 1;
            if image_alpha == 1 then other.state = WEB_TUT3.IDLE;
        }
        
        with(obj_atom)
        {
            image_alpha -= .05;
            if image_alpha &lt; 0 then image_alpha = 0;
        }              
        break;
        
    case WEB_TUT3.ISO_OUT_DOP_TRIGGER_IN:
    
        var atom_done = scr_tut_fade_in(obj_atom);        
        with(obj_atom_dop) visible = false;        
        var iso_done = scr_tut_fade_out(obj_tut3_isotopes);
        var trig_done = scr_tut_fade_in(obj_tut3_trigger);        
        var text_done = scr_tut_fade_out(obj_tut3_text);
        if atom_done and iso_done and trig_done and text_done
        then state = WEB_TUT3.BRING_IN_NEXT_TEXT;
        break;
        
    case WEB_TUT3.BRING_IN_NEXT_TEXT:
    
        with(obj_tut3_text2)
        {          
            event_user(0);
            depth = other.depth - 1;
        }
        
        obj_tut1_ok.click_handler = scr_tut3_web_click2;        
        state = WEB_TUT3.IDLE;
        break;
        
    case WEB_TUT3.POINTER_FADE_OUT:
        with(pointer)
        {
            image_alpha -= .08;
            if image_alpha &lt; 0 
            {
                x = xstart;
                y = ystart;
                visible = false;
                image_alpha = 0;
            }
           // if image_alpha == 0 then other.state = WEB_TUT1.RESET_FADE_IN;
        }
        break;
    case WEB_TUT3.POINTER_FADE_IN:
        with(pointer)
        {
            y = ystart;
            x = xstart;
            visible = true;
            image_alpha += .08;
            if image_alpha &gt; 1 then image_alpha = 1;
            
            if image_alpha == 1 
            {
                other.alarm[5] = 12; //START CLICK
                other.state = WEB_TUT2.IDLE;           
            }
        }
        break;
}
